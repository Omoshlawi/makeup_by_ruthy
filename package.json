    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "html-pdf": "^3.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.7",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "semver": "^7.6.3",
    "sharp": "^0.33.4",
    "slash": "^5.1.0",
    "slugify": "^1.6.6",
    "swagger-jsdoc": "^6.2.8",
